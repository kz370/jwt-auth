{
    "name": "kz370/jwt-auth",
    "description": "Secure JWT access token + refresh token authentication for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Achievers Dashboard",
            "email": "dev@achievers.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Kz370\\JwtAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kz370\\JwtAuth\\JwtAuthServiceProvider"
            ],
            "aliases": {
                "JwtAuth": "Kz370\\JwtAuth\\Facades\\JwtAuth"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
